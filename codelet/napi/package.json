{
  "name": "@sengac/codelet-napi",
  "version": "0.1.0",
  "description": "NAPI-RS bindings for codelet AI agent",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "codelet-napi",
    "targets": [
      "aarch64-apple-darwin",
      "x86_64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "x86_64-unknown-linux-gnu",
      "aarch64-pc-windows-msvc",
      "x86_64-pc-windows-msvc"
    ],
    "packageName": "@sengac/codelet-napi"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --esm",
    "build:debug": "napi build --platform --esm",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal -t codelet-napi",
    "version": "napi version"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "devDependencies": {
    "@napi-rs/cli": "^3.5.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sengac/fspec"
  },
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "@sengac/codelet-napi-darwin-arm64": "0.1.0",
    "@sengac/codelet-napi-darwin-x64": "0.1.0",
    "@sengac/codelet-napi-linux-arm64-gnu": "0.1.0",
    "@sengac/codelet-napi-linux-x64-gnu": "0.1.0",
    "@sengac/codelet-napi-win32-arm64-msvc": "0.1.0",
    "@sengac/codelet-napi-win32-x64-msvc": "0.1.0"
  }
}